<div class="teams box">
  <h4>Teams</h4>
  {{#each model as |org|}}
    <img class="w2 h2 w3-ns h3-ns mb2" alt={{org.name}} src={{org.image}}>
  {{/each}}
</div>
<div class="channels box">
  <h4>Channels</h4>
  <ul class="tl">
    {{#each model as |channel|}}
      {{#link-to 'channel.messages' channel}}
        <li class="flex pb2">{{channel.name}}</li>
      {{/link-to}}
    {{/each}}
  </ul>
</div>
{{outlet}}